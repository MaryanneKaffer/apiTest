generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Order {
  id              Int       @id @default(autoincrement())
  date            String?
  type            String?
  contact         String?
  corporateName   String?
  phone           String?
  address         String?
  city            String?
  state           String?
  cep             String?
  cpfCnpj         String?
  ie              String?
  district        String?
  payment         String?
  email           String?
  deliveryAddress String?
  code            String?
  qnt             String?
  size            String?
  description     String?
  cost            String?
  total           String?
  createdAt       DateTime  @default(now())
}


